    "@types/puppeteer": "^5.4.6",
    "agenda": "^4.3.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "firebase": "^9.10.0",
    "handlebars": "^4.7.7",
    "handlebars-dateformat": "^1.1.3",
    "localtunnel": "^2.0.2",
    "moment": "^2.29.4",
    "mongodb": "^4.10.0",
    "node-html-parser": "^6.1.0",
    "node-telegram-bot-api": "^0.59.0",
    "nodemon": "^2.0.20",
    "puppeteer": "^18.0.5",
    "puppeteer-core": "^18.0.5",
    "randomstring": "^1.2.2",
